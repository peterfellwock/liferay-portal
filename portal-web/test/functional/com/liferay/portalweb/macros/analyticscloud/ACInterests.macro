definition {

	macro assertNoCurrentInterests {
		AssertVisible(
			key_cardTitle = "Interests",
			locator1 = "ACCards#NO_RESULTS_TITLE");
	}

	macro viewInterests {
		var key_interestName = "${interestName}";

		AssertTextEquals(
			locator1 = "ACInterests#INTERESTS_TABLE",
			value1 = "${interestName}");
	}

}